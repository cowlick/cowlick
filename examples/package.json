{
  "name": "cowlick-sample",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "build": "cowlick-kag-compiler ./scenario/first.ks -o ./script && tsc -p . && npm run scan:assets",
    "start": "akashic-sandbox .",
    "scan:assets": "npm run copy:images && akashic scan asset",
    "copy:images": "shx cp ../docs/images/cowlick.png ./image",
    "bundle": "akashic export html -o ../dist -M -b && shx cp ../docs/images/favicon.ico ../dist"
  },
  "dependencies": {
    "@akashic-extension/akashic-label": "2.0.3",
    "@akashic-extension/akashic-timeline": "2.1.0",
    "@cowlick/config": "^0.8.0",
    "@cowlick/core": "^0.8.0",
    "@cowlick/engine": "^0.8.0",
    "@pocketberserker/akashic-pagination": "0.0.4",
    "@xnv/akashic-scrollable": "0.0.4"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "1.3.2",
    "@akashic/akashic-engine": "2.3.7",
    "@akashic/akashic-sandbox": "0.13.30",
    "@cowlick/kag-compiler": "^0.8.0",
    "@types/node": "9.4.7",
    "shx": "0.3.2",
    "typescript": "3.0.1"
  }
}
