{
  "name": "@cowlick/analyzer-plugin-script-encoder",
  "version": "0.9.0",
  "description": "script encoder plugin for cowlick analyzer",
  "main": "lib/index.js",
  "bin": {
    "cowlick-analyzer-plugin-script-encoder": "./bin/run"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p ./tsconfig.cli.json",
    "test": "jest"
  },
  "author": "pocketberserker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowlick/cowlick.git"
  },
  "dependencies": {
    "escodegen": "1.11.0",
    "estraverse": "4.2.0",
    "msgpack5": "4.2.0"
  },
  "devDependencies": {
    "@types/escodegen": "0.0.6",
    "@types/esprima": "4.0.2",
    "@types/estraverse": "0.0.6",
    "@types/jest": "23.3.1",
    "@types/msgpack5": "3.4.1",
    "@types/node": "9.4.7",
    "esprima": "4.0.1",
    "jest": "23.5.0",
    "power-assert": "1.6.0",
    "ts-jest": "23.1.4",
    "typescript": "3.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)test)\\.ts$",
