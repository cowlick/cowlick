{
  "name": "@cowlick/kag-compiler",
  "version": "0.7.0",
  "description": "cowlick kag compiler",
  "main": "lib/index.js",
  "bin": {
    "cowlick-kag-compiler": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "resources"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p ./tsconfig.cli.json && npm run pegjs",
    "test": "mocha --compilers ts:espower-typescript/guess test/*.ts",
    "pegjs": "pegjs --output ./resources/kag.js ./resources/kag.pegjs"
  },
  "author": "pocketberserker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowlick/cowlick.git"
  },
  "dependencies": {
    "@cowlick/analyzer": "^0.7.0",
    "@cowlick/core": "^0.7.0",
    "acorn": "5.7.1",
    "commandpost": "1.3.0",
    "estraverse": "4.2.0",
    "ora": "3.0.0"
  },
  "devDependencies": {
    "@akashic/akashic-engine": "2.3.5",
    "@types/acorn": "4.0.3",
    "@types/escodegen": "0.0.6",
    "@types/esprima": "4.0.2",
    "@types/estraverse": "0.0.6",
    "@types/glob": "5.0.35",
    "@types/mocha": "5.2.4",
    "@types/node": "9.4.7",
    "@types/ora": "1.3.4",
    "espower-typescript": "9.0.0",
    "esprima": "4.0.1",
    "mocha": "5.2.0",
    "pegjs": "0.10.0",
    "power-assert": "1.6.0",
    "typescript": "2.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
